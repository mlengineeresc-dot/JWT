//find prime 
function isPrime(n){
    if(n<=1) return false
    for(let i=2;i<n;i++){
        if(n%i===0){
            return false
        }
    }return true
    
}
// console.log(isPrime(6))

//find next prime 
function nextPrime(n){
    let num=n+1;
    while(true){
        if(isPrime(num)) return num
        num++
    }
    
}
console.log(nextPrime(4))


//Count vowels in substring

function vowelsInSubstring(str,start,end){
    let vowels="aeiou"
    let subString=str.substring(start,end)
    let count=0;
    
    for(let char of subString){
        if(vowels.includes(char)) count++
    }
    return count
}
console.log(vowelsInSubstring("javascript",4,10))
