const arr=[1,2,0,4,0,0,3,0,5,8] 

let index=0;
for(let i=0;i<arr.length;i++){
    if(arr[i]!==0){
        console.log("arr[i] here",arr[i])
        arr[index]=arr[i]
        console.log("after swapping",arr[index])
        index++;
    }
}

for (let i=index;i<arr.length;i++){
    arr[i]=0;
}

console.log(arr)
